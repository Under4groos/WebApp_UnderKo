<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] | UnderKo</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/WebApp_UnderKo.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/zip_css_combine.min.css" asp-append-version="true" />
     
    <link rel="icon" type="image/x-icon" href="/favicon.ico">



    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Major+Mono+Display&display=swap" rel="stylesheet">



    <link href="https://fonts.googleapis.com/css2?family=Gruppo&family=Major+Mono+Display&family=Orbitron:wght@400..900&family=Saira+Condensed:wght@100;200;300;400;500;600;700;800;900&family=Saira:ital,wght@0,100..900;1,100..900&family=Space+Mono:ital,wght@0,400;0,700;1,400;1,700&display=swap" rel="stylesheet">








    <meta name="google-site-verification" content="8q5RXKtrsVOpgjCJBYquf4AwEZzDy5o4If6pRYTV7M4" />

    <meta property="og:type" content="website">
    <meta property="og:url" content="http://underko.ru">
    <meta property="og:title" content="underko.ru">
    <meta property="og:description" content="Ничего лишнего, только КОТ">
    <meta property="og:image" content="https://i.imgur.com/15xjAbe.png">
</head>

<body style="background-image: linear-gradient(to right, rgba(10,10,10, 0.95) 0 100%), url('');
            background-position: center;
            background-repeat: no-repeat;
            background-size: cover;">

    <svg class="d-none" style="position:absolute; z-index:-9; height:100vh; width:100%; opacity:var(--opacity-svg-background)" viewBox="0 0 1317 808" fill="none" xmlns="http://www.w3.org/2000/svg">
        <path d="M515.92 553.62C665.01 652.23 737.37 748.83 852.83 769.62C951.01 787.31 1032.35 791.41 1148.66 703.88C1381.09 528.97 1268.56 242.88 1203.43 155.67C897.44 -254 841.55 456.52 683.79 377.54C633.13 352.17 595.25 282.54 497.14 179.14C417 94.66 333.6 58.94 259.56 65.91C129.07 78.19 -61.91 468 74.53 524.28C158.33 558.86 297.21 409 515.92 553.62Z" stroke="url(#paint0_linear_1_3)" stroke-width="125" stroke-miterlimit="10" />
        <defs xmlns="http://www.w3.org/2000/svg">
            <linearGradient xmlns="http://www.w3.org/2000/svg" id="paint0_linear_1_3" x1="1861" y1="-493" x2="-993.5" y2="179" gradientUnits="userSpaceOnUse">
                <stop stop-color="#161616" stop-opacity="0" />
                <stop offset="0.3" stop-color="#ffffff" />
                <stop offset="1" stop-color="#212121" stop-opacity="0" />
            </linearGradient>
        </defs>
    </svg>
    <button onclick="document.body.scrollTop = 0;document.documentElement.scrollTop = 0;" class="btn btn-dark button-page-up">
        <svg xmlns="http://www.w3.org/2000/svg" class="text-muted" width="20px" height="20px" viewBox="0 0 24 24" fill="none">
            <path d="M5 15L10 9.84985C10.2563 9.57616 10.566 9.35814 10.9101 9.20898C11.2541 9.05983 11.625 8.98291 12 8.98291C12.375 8.98291 12.7459 9.05983 13.0899 9.20898C13.434 9.35814 13.7437 9.57616 14 9.84985L19 15" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" />
        </svg>
    </button>

    <header class="blured header">
        <nav class="navbar  navbar-expand-sm navbar-toggleable-sm border-bottom-w navbar-light box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand text-muted" asp-area="" asp-page="/Index">UnderKo</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                        data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <svg xmlns='http://www.w3.org/2000/svg' width="30" height="30" viewBox='0 0 30 30'><path class="text-muted" stroke-linecap='round' stroke-miterlimit='10' stroke-width='2' d='M4 7h22M4 15h22M4 23h22' /></svg>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item dropdown">
                            <a class="nav-link  text-muted dropdown-toggle" href="#" data-bs-toggle="dropdown" aria-expanded="false">GitHub</a>
                            <ul class="header dropdown-menu dropdown-menu-d">
                                @foreach (var item in new (string, string)[] {
                                ("GitHubProjects", "GitHub"),

                                                    })
                                {
                                    <li><a class=" dropdown-item-d text-muted" asp-page="/@item.Item1">@item.Item2</a></li>
                                }


                            </ul>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link  text-muted dropdown-toggle" href="#" data-bs-toggle="dropdown" aria-expanded="false">Web Api</a>
                            <ul class="header dropdown-menu dropdown-menu-d">
                                @foreach (var item in new (string, string)[] {
                                ("apilist" , "Api"),

                                                    })
                                {
                                    <li><a class=" dropdown-item-d text-muted" asp-page="/@item.Item1">@item.Item2</a></li>
                                }


                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link  text-muted dropdown-toggle" href="#" data-bs-toggle="dropdown" aria-expanded="false">Tools</a>
                            <ul class="header dropdown-menu dropdown-menu-d">
                                @foreach (var item in new (string, string)[] {
                                ("Converters" , "Converters"),
                                ("YouTubeDownloader" , "YouTubeDownloader"),
                                                    })
                                {
                                    <li><a class=" dropdown-item-d text-muted" asp-page="/@item.Item1">@item.Item2</a></li>
                                }


                            </ul>
                        </li>
                        @foreach (var item in new (string, string)[]
                        {
                        ("MyProjects", "Projects"),
                        ("Donate" , "Donate"),
                        })
                        {

                            <li class="nav-item">
                                <a class="nav-link text-muted" asp-page="/@item.Item1">@item.Item2</a>
                            </li>
                        }

                    </ul>
                </div>
            </div>
        </nav>
    </header>



    <div class="container ">

        <main role="main">

            @RenderBody()

        </main>
    </div>


    <footer class="footer border-top-w blured">
        <div class="container text-muted">
            &copy; 2024 - UnderKo.ru - <a class="text-reset" asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/zip_scripts_combine.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

